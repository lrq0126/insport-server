<view class="login"><view class="content"><view class="header"><image src="{{logoImage}}"></image><text>泓钊课后服务</text></view><view class="main"><w-input bind:input="__e" vue-id="1" type="text" maxlength="11" placeholder="用户名/电话" value="{{mynote.phone}}" data-event-opts="{{[['^input',[['__set_model',['$0','phone','$event',[]],['mynote']]]]]}}" bind:__l="__l"></w-input><w-input bind:input="__e" vue-id="2" type="password" maxlength="11" placeholder="密码" value="{{mynote.password}}" data-event-opts="{{[['^input',[['__set_model',['$0','password','$event',[]],['mynote']]]]]}}" bind:__l="__l"></w-input></view><w-button vue-id="3" text="登 录" rotate="{{isRotate}}" data-event-opts="{{[['tap',[['startLogin']]]]}}" bindtap="__e" bind:__l="__l"></w-button><view class="other_login cuIcon"><view class="login_icon"><view data-event-opts="{{[['tap',[['login_weixin',['$event']]]]]}}" class="cuIcon-weixin" style="font-size:64rpx;" bindtap="__e"></view></view><view class="login_icon"><view data-event-opts="{{[['tap',[['login_weibo',['$event']]]]]}}" class="cuIcon-weibo" style="font-size:64rpx;" bindtap="__e"></view></view><view class="login_icon"><view data-event-opts="{{[['tap',[['login_github',['$event']]]]]}}" class="cuIcon-github" style="font-size:64rpx;" bindtap="__e"></view></view></view><view class="footer"><navigator url="forget" open-type="navigate">找回密码</navigator><text>|</text><navigator url="register" open-type="navigate">注册账号</navigator></view></view></view>