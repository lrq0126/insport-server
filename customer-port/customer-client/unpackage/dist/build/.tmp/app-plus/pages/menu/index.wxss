
.container{border-top: 1px solid #f0f0f0;}
.swiper-box{height: 124px;}
.banner{position: relative;}
.banner-image{width: 100%;}
.page-body {display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;}
.nav {display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;width: 100%;}
.nav-left {width: 180rpx;background-color: #f7f7f7;}
.nav-left-item {height: 86rpx;border-bottom: solid 1px #ececec;font-size: 28rpx;display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-align: center;-webkit-align-items: center;-ms-flex-align: center;align-items: center;-webkit-box-pack: center;-webkit-justify-content: center;-ms-flex-pack: center;justify-content: center;color: #666;}
.nav-left-item:last-child{border-bottom: none;}
.active {color: #333;background-color: #fff;font-weight: 700;}
.nav-right {width: 570rpx;background-color: #fff;}
.nav-right-item {width: 100%;font-size: 28rpx;padding: 0 26rpx;-webkit-box-sizing: border-box;box-sizing: border-box;}
.item-header{padding-top: 36rpx;font-size: 24rpx;font-weight: 700;line-height: 24rpx;}
.item-state,.item-en,.item-default{font-size: 20rpx;line-height: 20rpx;color: #999;}
.item-state{margin-top: 10rpx;}
.item-li{padding: 22rpx 0;position: relative;border-bottom: 1px solid #f1f1f1;height: 134rpx;}
.nav-right-item .item-li:last-child{border-bottom: none;}
.item-img{width: 134rpx;height: 134rpx;border-radius: 8rpx;}
.item-text{display: inline-block;vertical-align: top;margin-left: 16rpx;}
.item-title{font-size: 28rpx;line-height: 28rpx;font-weight: 700;}
.item-en{margin: 10rpx 0 8rpx 0;}
.item-price{font-size: 28rpx;line-height: 28rpx;font-weight: 700;margin-top: 28rpx;}
.plus-filled{position: absolute;bottom: 16rpx;right: 0;color: #81aad2;}
.fixed{position: fixed;top: 0;left: 206rpx;background: #fff;}
.mask {position: fixed;z-index: 99;top: 0;right: 0;bottom: 0;left: 0;background-color: rgba(0, 0, 0, .3);}
.popup {position: fixed;z-index: 999;background-color: #fff;-webkit-box-shadow: 0 0 30rpx rgba(0, 0, 0, .1);box-shadow: 0 0 30rpx rgba(0, 0, 0, .1);display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-orient: vertical;-webkit-box-direction: normal;-webkit-flex-direction: column;-ms-flex-direction: column;flex-direction: column;
-webkit-box-align: center;
-webkit-align-items: center;
    -ms-flex-align: center;
        align-items: center;width: 690rpx;height: 920rpx;border-radius: 10rpx;top: 50%;left: 50%;-webkit-transform: translate(-50%, -50%);-ms-transform: translate(-50%, -50%);transform: translate(-50%, -50%);-webkit-box-sizing: border-box;box-sizing: border-box;overflow: hidden;}
.popup-header,.popup-middle,.popup-footer{width: 100%;}
.popup-header{height: 288rpx;position: relative;background: url(https://s2.luckincoffeecdn.com/luckywebrm/images/index/cooperation/part5_picture2.png) center;}
.popup-title,.popup-en{color: #fff;margin-left: 20rpx;vertical-align: bottom;}
.popup-title{font-size: 34rpx;line-height: 54rpx;margin-top: 190rpx;}
.popup-en{font-size: 20rpx;line-height: 20rpx;}
.popup-close{color: #fff;font-size: 56rpx;position: absolute;top: 20rpx;right: 20rpx;}
.popup-middle{height: 400rpx;-webkit-box-sizing: border-box;box-sizing: border-box;padding: 10rpx 30rpx 0;overflow: auto;}
.popup-footer{height: 232rpx;border-top: 1px solid #f1f1f1;}
.popup-price{display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-pack: justify;-webkit-justify-content: space-between;-ms-flex-pack: justify;justify-content: space-between;padding: 32rpx 28rpx 24rpx 38rpx;height: 120rpx;-webkit-box-sizing: border-box;box-sizing: border-box;border-bottom: 1px solid #f1f1f1;}
.text-main{font-size: 32rpx;line-height: 32rpx;color: #222;margin-bottom: 15rpx;}
.text-small{font-size: 20rpx;color: #333;line-height: 20rpx;}
.numbox{display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;}
.numbox-minus,.numbox-plus{font-size: 56rpx;line-height: 68rpx;}
.numbox-minus{color: #91b5d9;background-color: #fff;}
.numbox-value{line-height: 68rpx;margin: 0 16rpx;color: #91b5d9;font-weight: 700;min-width: 20rpx;}
.numbox-plus{color: #fff;color: #91b5d9;}
.btn-group{display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-box-pack: end;-webkit-justify-content: flex-end;-ms-flex-pack: end;justify-content: flex-end;margin-top: 26rpx;}
.btn{height: 60rpx;line-height: 58rpx;padding: 0;margin: 0;font-size: 24rpx;text-align: center;color: #fff;border-radius: 0;margin-right: 14rpx;}
.first-btn{width: 206rpx;border: 1px solid #dc5a00;background-color: #e06e11;}
.two-btn{width: 170rpx;border: 1px solid #6c9ccd;background-color: #f3f3f3;color: #73a1cf;}
.three-btn{width: 180rpx;border: 1px solid #6c9ccd;background-color: #7ca7d2;}
.btn:after{border: none;}
.sort{display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;margin-top: 30rpx;}
.sort-label{width: 120rpx;line-height: 52rpx;}
.sort-select{width: 120rpx;height: 50rpx;text-align: center;border-radius: 30rpx;border: 1px solid #e3dbd3;color: #e3dbd3;font-size: 24rpx;line-height: 50rpx;margin-right: 15rpx;}
.sort-select.active{background-color: #e3dbd3;color: #fff;font-weight: 400;}
.describe{border-top: 1px solid #f1f1f1;margin-top: 30rpx;padding: 30rpx 0 10rpx;}
.describe-title{margin-bottom: 10rpx;}
.describe-text{color: #666;font-size: 24rpx;}

