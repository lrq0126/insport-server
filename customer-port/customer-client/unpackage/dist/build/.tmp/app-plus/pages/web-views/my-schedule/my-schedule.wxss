
page {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	background-color: #efeff4
}
view {
	font-size: 28rpx;
	line-height: inherit
}
.example {
	padding: 0 30rpx 30rpx
}
.example-title {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	font-size: 32rpx;
	color: #464e52;
	padding: 30rpx 30rpx 30rpx 50rpx;
	margin-top: 20rpx;
	position: relative;
	background-color: #fdfdfd;
	border-bottom: 1px #f5f5f5 solid
}
.example-title__after {
	position: relative;
	color: #031e3c
}
.example-title:after {
	content: '';
	position: absolute;
	left: 30rpx;
	margin: auto;
	top: 0;
	bottom: 0;
	width: 6rpx;
	height: 32rpx;
	background-color: #ccc
}
.example .example-title {
	margin: 40rpx 0
}
.example-body {
	padding: 30rpx;
	background: #fff
}
.example-info {
	padding: 30rpx;
	color: #3b4144;
	background: #fff
}
.calendar-content {
	padding-bottom: 100rpx;
	font-size: 26rpx;
	margin-top: 6rpx;
}
.calendar-content-active {
	/* padding-bottom: 450upx; */
}
.calendar-tags-group {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}
.calendar-tags {
	width: 100%;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.calendar-tags-item {
	padding: 20rpx 20rpx;
	border: 1px rgba(0, 0, 0, 0.2) solid;
	color: #333;
	border-radius: 10rpx;
	text-align: center;
	margin: 10rpx;
	background: #f8f8f8;
}
.calendar-tags-item:active {
	background: #f8f8f8;
}
.checked .calendar-tags-item {
	background: rgb(0, 122, 255);
	color: #fff;
	border: 1px rgba(0, 0, 0, 0.1) solid;
}
.calendar-button {
	font-weight: bold;
	font-size: 32rpx;
}
.calendar-button-groups {
	position: absolute;
	width: 100%;
	bottom: 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.calendar-button-confirm {
	width: 50%;
	margin: 10rpx;
	border: 1px #eee solid;
	font-size: 32rpx;
}
.calendar-button-confirm:after {
	border: none;
}
.calendar-box {
	position: fixed;
	bottom: 0;
	background: #fff;
	color: #444;
	line-height: 1.5;
	width: 100%;
	-webkit-transition: all 0.3s;
	-o-transition: all 0.3s;
	transition: all 0.3s;
	-webkit-transform: translateY(355rpx);
	    -ms-transform: translateY(355rpx);
	        transform: translateY(355rpx);
	/* background: #f5f5f5; */
}
.calendar-active {
	-webkit-transform: translateY(-50rpx);
	    -ms-transform: translateY(-50rpx);
	        transform: translateY(-50rpx);
	/* transform: translateY(0); */
}
.calendar-info-header {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	padding: 20rpx 30rpx;
	padding-left: 0;
	border-top: 1px #eee solid;
	border-bottom: 1px #eee solid;
}
.calendar-title {
	/* height: 80upx; */
	font-weight: bold;
	color: #666;
	font-size: 32rpx;
	border-left: 2px #0d9ebb solid;
	padding-left: 20rpx;
	margin: 0 20rpx;
}
.calendar-info {
	overflow-y: scroll;
	height: 460rpx;
	/* height: 260upx; */
	padding: 0 30rpx;
}
.uni-list-cell:after {
	left: 1px !important;
}

