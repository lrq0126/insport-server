.scanCode-schoolOver {
  height: 100%;
}
.scanCode-schoolOver .example-body {
    height: 100%;
    padding: 30rpx;
    background: #fff;
}
.scanCode-schoolOver .example-info {
    padding: 30rpx;
    color: #3b4144;
    background: #fff;
}
.scanCode-schoolOver .example-body {
    padding: 20rpx 0;
}
.scanCode-schoolOver .tag-view {
    margin: 50rpx 36rpx;
    display: inline-block;
    width: 90%;
    text-align: center;
}
.scanCode-schoolOver .uni-tag {
    font-size: 34rpx;
    padding: 50rpx 30rpx;
}
.scanCode-schoolOver .uni-tag--success,
  .scanCode-schoolOver .uni-tag--primary {
    width: 100%;
}
.scanCode-schoolOver .uni-tag--default,
  .scanCode-schoolOver .uni-tag--warning {
    width: 50%;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
        -ms-flex-pack: center;
            justify-content: center;
}
.scanCode-schoolOver .uni-tag--default {
    background-color: #ddd;
}
.scanCode-schoolOver .uni-countdown__number {
    font-size: 40rpx !important;
}

